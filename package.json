{
  "name": "react-redux-basic",
  "version": "1.3.0",
  "description": "React + Redux + Jest + Enzyme basic example",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "test:update": "jest --verbose --updateSnapshot",
    "test:log": "jest --updateSnapshot --coverage --verbose",
    "dev": "webpack-dev-server --config webpack.config.js"
  },
  "keywords": [
    "react",
    "jsx",
    "redux",
    "webpack",
    "babel",
    "es6",
    "middleware",
    "axios",
    "jest",
    "enzyme"
  ],
  "author": "I Nengah Januartha",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.2",
    "node-sass": "^4.9.2",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^3.6.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^2.8.2"
  },
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js",
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "babel-jest"
    },
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__test__/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  }
}
